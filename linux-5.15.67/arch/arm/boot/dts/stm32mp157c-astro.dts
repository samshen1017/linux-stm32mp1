/dts-v1/;

#include "stm32mp157.dtsi"
#include "stm32mp15xc.dtsi"
#include "stm32mp15-pinctrl.dtsi"
#include "stm32mp15xxac-pinctrl.dtsi"
#include "stm32mp157c-astro.dtsi"

/ {
	model = "STMicroelectronics STM32MP157C-ASTRO Board";
	compatible = "st,stm32mp157c-astro", "st,stm32mp157";
	
	aliases {
		serial8 = &uart8;
	};
	
	chosen {
		stdout-path = "serial8:115200n8";
	};
};

&cryp1 {
	status = "okay";
};

